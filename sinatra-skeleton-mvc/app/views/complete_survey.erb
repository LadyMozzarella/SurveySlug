<h1><%= @survey.name%></h1>
<form>
  <% @survey.questions.each do |question| %>
    <li>
      <div>
        <h3><%= question.prompt %></h3>
        <ul>
          <% question.options.each do |option| %>
            <label for="option"><%= option.name %>
            <input type="radio" name="option"></label>
          <% end %>
        </ul>
      </div>
    </li>
  <% end %>
  <button>Submit Survey</button>
</form>